    <script>
        var btns = document.querySelectorAll(".btn");
      
        btns.forEach(function(btn) {
          btn.addEventListener("click", function() {
            // Find the corresponding content and line for the clicked button
            var content = btn.nextElementSibling;
            var line = btn.querySelector(".line");
      
            // Toggle the visibility of content
            content.classList.toggle("hidden");
      
            // Toggle the visibility of the line
            line.style.visibility = (content.classList.contains("hidden")) ? "visible" : "hidden";
            
          });
        });
      </script>

